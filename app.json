{
  "expo": {
    "name": "Arctic Wolves",
    "slug": "ACWolvesApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ArcticWolves.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arcticwolves.dash"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "package": "com.arcticwolves.dash",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Arctic Wolves",
      "shortName": "AC Wolves",
      "description": "Arctic Wolves team management app",
      "themeColor": "#0A0A0F",
      "backgroundColor": "#0A0A0F"
    }
  }
}
